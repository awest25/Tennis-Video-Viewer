{
  "hosting": {
    "source": ".",
    "ignore": ["firebase.json","**/.*","**/node_modules/**"],
    "cleanUrls": true,
    "rewrites": [
      {
        "destination": "/matches/[slug]/index.html",
        "regex": "^/user/([^/]+?)(?:/)?$"
      }
    ],
    "frameworksBackend": {
      "region": "us-central1"
    }
  },
  "functions": {
    "source": "app/functions"
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true,
      "port": 4000
    }
  }
}
